<?xml version="1.0"?>
  <root main_tree_to_execute = "MainTree" >
     <BehaviorTree ID="MainTree">
        <Sequence name="root_sequence">
            <Action ID ="UpdateParameter" service_name="update_param" topic="/arm_param_server" data_type="double"/>
            <Action ID ="UpdateParameter" service_name="update_param" topic="/gripper_param_server" data_type="string"/>

            <Action ID ="FindObjects" container_A="-0.35;0.55;0.78;0;0.707;0;0.707;" container_B="0.35;0.55;0.78;0;0.707;0;0.707" marker_id="2" frame_id="world" marker="{marker}" container="{container}"/>
            <Action ID ="ComputePath" goal_name="home" target_type="Name" replan_times="3" plan="{plan}"/>
            <Action ID ="ExecuteGripperTrajectory" action_name="open" result="{gripper_exection_result}"/>
            <Action ID ="ExecuteTrajectory" plan="{plan}" result="{exection_result}"/>
            <Action ID ="UpdateGoalForArm" service_name="update_arm_goal" step="T2S1arm" goal_frame_id="world" target="{container}" goal="{arm_goal}"/>
            <Action ID ="ComputePath" goal="{arm_goal}" target_type="Pose" replan_times="3" plan="{plan}"/>
            <Action ID ="ExecuteTrajectory" plan="{plan}" result="{exection_result}"/>
            <Action ID ="ExecuteGripperTrajectory" step="T2S1gripper" result="{gripper_exection_result}"/>
            <Action ID ="UpdateGoalForArm" service_name="update_arm_goal" step="T2S2arm" goal_frame_id="world" target="{container}" goal="{arm_goal}"/>
            <Action ID ="ComputePath" goal="{arm_goal}" target_type="Cartesian" planner_id ="RRTConnectkConfigDefault" replan_times="3" plan="{plan}"/>
            <Action ID ="ExecuteTrajectory" plan="{plan}" result="{exection_result}"/>
            <Action ID ="ExecuteGripperTrajectory" step="T2S2gripper" result="{gripper_exection_result}"/>
        </Sequence>
     </BehaviorTree>
 </root>