 <root BTCPP_format="4">
     <BehaviorTree ID="MainTree">
        <Sequence>
            <PrintValue message="start"/>
            <SetBlackboard output_key="eee" value="world"/>
            <PrintValue message="sleep completed"/>
            <DetectArucoMarkerClient marker_id="42" sub_topic_name="/marker" timeout="5000" marker_pose="{marker_pose}" error_code_id="{detect_marker_error_code}"/>
            <Fallback>
                <Timeout msec="1500">
                   <SetBlackboard output_key="eee" value="world"/>
                </Timeout>
                <PrintValue message="sleep aborted"/>
            </Fallback>
        </Sequence>
     </BehaviorTree>
 </root>
